/* pages/orderList/details/index.wxss */
.pay_type{
  background: #F1EEE3;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: content-box;
  padding: 25rpx 119rpx 19rpx 78rpx;
}
.pay_type>view{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.pay_type .pay_state_left{
  align-items: flex-start;
}
.pay_title{
  font-size: 30rpx;
  line-height: 42rpx;
  color: #454343;
}
.pay_date{
  font-size: 20rpx;
  line-height: 28rpx;
  color: #6B6B6B;
}
.pay_state_right image{
  height: 60rpx;
}



.goodsList{
  width: 100%;
  padding: 0 30rpx;
  background: #fff;
  margin-bottom: 20rpx;
}
.goodsList_no_use{
  padding: 0;
}
.goodsList:first-child{
  /* margin-top: 10rpx; */
}

.goodsTitle{
  position: relative;
  padding-top: 20rpx;
  line-height: 37rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.goodsTitle .name{
  margin-right: 35rpx;
}
.goodsTitle .discounts{
  position: absolute;
  top: 26rpx;
  right: 0;
  width: 90rpx;
  height: 30rpx;
  line-height: 26rpx;
  text-align: center;
  border-radius: 4rpx;
  border: 1px solid #977958;
}
.collect{
  padding-left: 56rpx;
}
.goodsItem{
  position: relative;
  padding: 20rpx 30rpx;
  border-bottom: 1rpx solid #c6c5c4;
}
.goodsItem:last-child{
  border-bottom: 0;
}
.goodsItem .right{
  width: 180rpx;
  height: 180rpx;
  overflow: hidden;
  border-radius: 10rpx;
}
.goodsItem .right image{
  width: 100%;
  height: 100%;
}
.goodsItem .left{
  flex:1;
  margin-left: 20rpx;
}
.goodsItem .left .tap{
  color: #CE9455;
}

.goodsItem .left .info{
  height: 60rpx;
  line-height: 30rpx;
  margin-bottom: 15rpx;
}
.select{
  color: #c5c5c5;
}
.quota{
  width: 93rpx;
  height: 30rpx;
  line-height: 30rpx;
  color: #fff;
  background: #977958;
  border-radius: 6rpx;
  opacity: 0.6;
  text-align: center;
}
/* 订单信息 */
.order_info{
  width: 100%;
  padding: 24rpx 30rpx 28rpx;
  background: #ffffff;
}
.order_info .order_title{
  font-size: 30rpx;
  line-height: 42rpx;
  color: #322E2A;
  letter-spacing:1px;
  margin-bottom: 30rpx;
}
.order_info_item{
  font-size: 24rpx;
  line-height: 33rpx;
  color: #6B6B6B;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-bottom: 30rpx;
}
.order_info_item:last-child{
  margin-bottom: 0;
}
.order_info_item .order_txt{
  width: 149rpx;
  margin-right: 6rpx;
}
.order_info_item .order_con{
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.copy_btn{
  margin-left: 30rpx;
  border: 1rpx solid #979797;
  font-size: 20rpx;
  line-height: 28rpx;
  color: #454343;
  text-align: center;
  padding: 0 10rpx;
}
/* 客服电话 */
.phone_btn{
  width: 100%;
  height: 70rpx;
  border-top: 1rpx solid #C6C5C4;
  background: #ffffff;
  display: flex;
  align-items: center;
  margin-bottom: 29rpx;
}
.phone_btn .pbone{
  width: 50%;
  height: 45rpx;
  border-right: 1rpx solid #DCDBDB;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #6B6B6B;
}
.phone_btn image{
  height: 34rpx;
  margin-right: 20rpx;
}

/* 底部按钮 */
.bottom_btn{
  width: 100%;
  height: 80rpx;
  display: flex;
}
.bottom_btn>view{
  width: 50%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  line-height: 45rpx;
  color: #454343;
  background: #ffffff;
}
.bottom_btn>.buy_order{
  color: #ffffff;
  background: #977958;
}
.time{
  margin-left: 20rpx;
}
.order_option{
  width: 100%;
  height: 95rpx;
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.order_option>view{
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.order_option .order_left>view{
  width: 107rpx;
  height: 55rpx;
  border: 1rpx solid #322E2A;
  font-size: 24rpx;
  line-height: 33rpx;
  color: #322E2A;
  display: flex;
  align-items: center;
  justify-content: center;
}
.order_option .order_left>view:first-child{
  margin-right: 20rpx;
}
.order_right image{
  width: 34rpx;
  height: 34rpx;
  margin-right: 20rpx;
}

.coupon_info{
  width: 100%;
  height: 305rpx;
  background: #ffffff;
  margin-bottom: 38rpx;
  padding: 37rpx 30rpx 32rpx 32rpx;

}
.coupon_item{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 31rpx;
}
.coupon_item:last-child{
  margin-bottom: 0;
}
.coupon_item .coupon_title{
  font-size: 30rpx;
  line-height: 42rpx;
  color: #322E2A;
}
.coupon_item .coupon_title{
  font-size: 28rpx;
  line-height: 32rpx;
  color: #6B6B6B;
}
.coupon_price_yes{
  color: #977958;
  font-size: 32rpx;
  line-height: 37rpx;
}

/* 二维码 */
.qr_warp{
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.7);
}
.qr_content{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 600rpx;
  height: 600rpx;
  background: #ffffff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-radius: 4rpx;
}
.qr_canvas{
  margin: 46rpx 0 29rpx;
  width: 356rpx;
  height: 356rpx;
  background: black;
  color: #ffffff;
}
.qr_title{
  font-size: 32rpx;
  line-height: 36rpx;
  color: #454343;
}
.qr_num{
  font-size: 30rpx;
  line-height: 42rpx;
  color: #333333;
}
