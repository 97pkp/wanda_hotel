/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  background: #F0F0F0;
}


/* 隐藏横向滚动条 */
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
image{
  display: block !important;
}