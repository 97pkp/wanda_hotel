/**app.wxss**/

/** colorUI样式组件 **/
@import "colorui/main.wxss";
@import "colorui/icon.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  background: #F0F0F0;
}
/* 隐藏横向滚动条 */
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
/* 字体大小 */
.f_s_50{
  font-size: 50rpx;
}
.f_s_36{
  font-size: 36rpx;
}
.f_s_32{
  font-size: 32rpx;
}
.f_s_30{
  font-size: 30rpx;
}
.f_s_28{
  font-size: 28rpx;
}
.f_s_26{
  font-size: 26rpx;
}
.f_s_24{
  font-size: 24rpx;
}
.f_s_20{
  font-size: 20rpx;
}
.f_s_18{
  font-size: 18rpx;
}
.d_x{
  display: flex;
}
/* 行高 */
.l_h_28{
  line-height: 28rpx;
}
/* 内边距 */
.p_t_14{
  padding-top: 14rpx;
}
.p_t_24{
  padding-top: 24rpx;
}
/* 外边距 */
.m_r_10{
  margin-right: 10rpx;
}
.m_r_20{
  margin-right: 20rpx;
}
.m_t_20{
  margin-top: 20rpx;
}
.m_t_24{
  margin-top: 24rpx;
}
.m_r_30{
  margin-right: 30rpx;
}
.m_l_30{
  margin-left: 30rpx;
}
.m_b_24{
  margin-bottom: 24rpx;
}
.m_t_40{
  margin-top: 40rpx;
}
image{
  display: block !important;
}
